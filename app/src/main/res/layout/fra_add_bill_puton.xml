<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusableInTouchMode="true"
    android:focusable="true"
    android:background="@color/white"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/ic_bt"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/txm"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etBarCode"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:singleLine="true"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:drawableLeft="@mipmap/ic_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/putOnDate"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvPutOnDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:drawableRight="@mipmap/ic_arrow_down"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:hint="@string/qxz"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:drawableLeft="@mipmap/ic_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/zhongliang"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/etWeight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:background="@null"
                    android:inputType="numberDecimal"
                    android:textColor="@color/txt_lv6"
                    android:textSize="@dimen/txt_size_14"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="1dp"
                    android:background="#efefef"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivAdd"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_up"/>
                    <View
                        android:layout_width="match_parent"
                        android:background="#efefef"
                        android:layout_height="1dp"/>
                    <ImageView
                        android:id="@+id/ivReduce"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_down"/>

                </LinearLayout>

            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/tph"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvPalletNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:inputType="phone"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_unused_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/spdh"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvProductCode"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_unused_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/goodsName"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvGoodsName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_unused_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/ic_bt"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/length"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/etLength"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:numeric="integer"
                    android:maxLength="7"
                    android:background="@null"
                    android:textColor="@color/txt_lv6"
                    android:textSize="@dimen/txt_size_14"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="1dp"
                    android:background="#efefef"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivAddLength"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_up"/>
                    <View
                        android:layout_width="match_parent"
                        android:background="#efefef"
                        android:layout_height="1dp"/>
                    <ImageView
                        android:id="@+id/ivReduceLength"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_down"/>

                </LinearLayout>

            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/ic_bt"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/width"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/etWidth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:numeric="integer"
                    android:background="@null"
                    android:maxLength="7"
                    android:textColor="@color/txt_lv6"
                    android:textSize="@dimen/txt_size_14"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="1dp"
                    android:background="#efefef"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivAddWidth"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_up"/>
                    <View
                        android:layout_width="match_parent"
                        android:background="#efefef"
                        android:layout_height="1dp"/>
                    <ImageView
                        android:id="@+id/ivReduceWidth"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_down"/>

                </LinearLayout>

            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/ic_bt"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/height"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/etHeight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:maxLength="7"
                    android:numeric="integer"
                    android:text=""
                    android:textColor="@color/txt_lv6"
                    android:textSize="@dimen/txt_size_14"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="1dp"
                    android:background="#efefef"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivAddHeight"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_up"/>
                    <View
                        android:layout_width="match_parent"
                        android:background="#efefef"
                        android:layout_height="1dp"/>
                    <ImageView
                        android:id="@+id/ivReduceHeight"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_down"/>

                </LinearLayout>

            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/ck"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvWmsWarehouseId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_unused_5dp"/>




            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/ic_bt"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/cw"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvWmsWarehouseDetailId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:drawableRight="@mipmap/ic_arrow_down"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:hint="@string/qxz"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_unused_5dp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/remarks"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etRemark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:maxLength="200"
                android:layout_marginBottom="50dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
        </LinearLayout>
    </ScrollView>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/space" />

    <TextView
        android:id="@+id/tvSave"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="12dp"
        android:text="@string/save"
        android:textColor="@color/main_color" />
</LinearLayout>