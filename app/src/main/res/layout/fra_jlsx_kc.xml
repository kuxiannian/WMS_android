<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/txm"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etBarCode"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="AWB"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etAwb"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/hwfl"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvGoodsType"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:drawableRight="@mipmap/ic_arrow_down"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:hint="@string/qxz"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/hwmc"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etGoodsName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/spdh"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etProductCode"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/ck"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvWmsWarehouseId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:drawableRight="@mipmap/ic_arrow_down"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:hint="@string/qxz"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/cw"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etWmsWarehouseDetailName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/stockState"
                android:textColor="@color/txt_lv6" />
            <TextView
                android:id="@+id/tvStockState"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:drawableRight="@mipmap/ic_arrow_down"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:hint="@string/qxz"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/rkrq"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tvInputDateStart"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/txt_lv6"
                    android:layout_marginTop="5dp"
                    android:textSize="@dimen/txt_size_14"
                    android:hint="@string/startPlace"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="20dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:background="@color/line"/>
                <TextView
                    android:id="@+id/tvInputDateEnd"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/txt_lv6"
                    android:layout_marginTop="5dp"
                    android:textSize="@dimen/txt_size_14"
                    android:hint="@string/endPlace"
                    android:padding="12dp"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/outputDate"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tvOutputDateStart"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/txt_lv6"
                    android:layout_marginTop="5dp"
                    android:textSize="@dimen/txt_size_14"
                    android:hint="@string/startPlace"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="20dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:background="@color/line"/>
                <TextView
                    android:id="@+id/tvOutputDateEnd"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/txt_lv6"
                    android:layout_marginTop="5dp"
                    android:textSize="@dimen/txt_size_14"
                    android:hint="@string/endPlace"
                    android:padding="12dp"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/inStockDayStart"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/tvInStockDayStart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:layout_weight="1"
					android:numeric="integer"
                    android:text=""
                    android:maxLength="7"
                    android:textColor="@color/txt_lv6"
                    android:textSize="@dimen/txt_size_14"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="1dp"
                    android:background="#efefef"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivAddStockDayStart"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_up"/>
                    <View
                        android:layout_width="match_parent"
                        android:background="#efefef"
                        android:layout_height="1dp"/>
                    <ImageView
                        android:id="@+id/ivReduceStockDayStart"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_down"/>

                </LinearLayout>

            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/inStockDayEnd"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/tvInStockDayEnd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
					android:numeric="integer"
                    android:maxLength="7"
                    android:text=""
                    android:textColor="@color/txt_lv6"
                    android:textSize="@dimen/txt_size_14"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="1dp"
                    android:background="#efefef"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivAddStockDayEnd"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_up"/>
                    <View
                        android:layout_width="match_parent"
                        android:background="#efefef"
                        android:layout_height="1dp"/>
                    <ImageView
                        android:id="@+id/ivReduceStockDayEnd"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/ic_arrow_down"/>

                </LinearLayout>

            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/updaterId_stock"
                android:textColor="@color/txt_lv6" />
            <EditText
                android:id="@+id/etUpdaterName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/txt_lv6"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/txt_size_14"
                android:padding="12dp"
                android:background="@drawable/bg_border_efefef_5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="12dp"
                android:textSize="15sp"
                android:text="@string/updateDate_stock"
                android:textColor="@color/txt_lv6" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="50dp"
                android:background="@drawable/bg_border_efefef_5dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tvUpdateDateStart"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/txt_lv6"
                    android:layout_marginTop="5dp"
                    android:textSize="@dimen/txt_size_14"
                    android:hint="@string/startPlace"
                    android:padding="12dp"
                    />
                <View
                    android:layout_width="20dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:background="@color/line"/>
                <TextView
                    android:id="@+id/tvUpdateDateEnd"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/txt_lv6"
                    android:layout_marginTop="5dp"
                    android:textSize="@dimen/txt_size_14"
                    android:hint="@string/endPlace"
                    android:padding="12dp"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/space" />

    <TextView
        android:id="@+id/tvCx"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="12dp"
        android:text="@string/cx"
        android:textColor="@color/main_color" />
</LinearLayout>